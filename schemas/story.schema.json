{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "kind",
      "id",
      "created_at",
      "updated_at",
      "story_type",
      "name",
      "current_state",
      "requested_by_id",
      "url",
      "project_id",
      "owner_ids",
      "labels"
    ],
    "properties": {
      "kind": {
        "$id": "#/items/properties/kind",
        "type": "string",
        "title": "The Kind Schema",
        "default": "",
        "examples": [
          "story"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          163810574
        ]
      },
      "created_at": {
        "$id": "#/items/properties/created_at",
        "type": "string",
        "title": "The Created_at Schema",
        "default": "",
        "examples": [
          "2019-02-08T03:12:33Z"
        ],
        "pattern": "^(.*)$"
      },
      "updated_at": {
        "$id": "#/items/properties/updated_at",
        "type": "string",
        "title": "The Updated_at Schema",
        "default": "",
        "examples": [
          "2019-02-08T03:12:33Z"
        ],
        "pattern": "^(.*)$"
      },
      "story_type": {
        "$id": "#/items/properties/story_type",
        "type": "string",
        "title": "The Story_type Schema",
        "default": "",
        "examples": [
          "feature"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "xxxstory"
        ],
        "pattern": "^(.*)$"
      },
      "current_state": {
        "$id": "#/items/properties/current_state",
        "type": "string",
        "title": "The Current_state Schema",
        "default": "",
        "examples": [
          "unscheduled"
        ],
        "pattern": "^(accepted|delivered|finished|started|rejected|planned|unstarted|unscheduled)$"
      },
      "requested_by_id": {
        "$id": "#/items/properties/requested_by_id",
        "type": "integer",
        "title": "The Requested_by_id Schema",
        "default": 0,
        "examples": [
          3143894
        ]
      },
      "url": {
        "$id": "#/items/properties/url",
        "type": "string",
        "title": "The Url Schema",
        "default": "",
        "examples": [
          "https://www.pivotaltracker.com/story/show/163810574"
        ],
        "pattern": "^(.*)$"
      },
      "project_id": {
        "$id": "#/items/properties/project_id",
        "type": "integer",
        "title": "The Project_id Schema",
        "default": 0,
        "examples": [
          2256970
        ]
      },
      "owner_ids": {
        "$id": "#/items/properties/owner_ids",
        "type": "array",
        "title": "The Owner_ids Schema"
      },
      "labels": {
        "$id": "#/items/properties/labels",
        "type": "array",
        "title": "The Labels Schema"
      }
    }
  },
  "additionalProperties": false
}
