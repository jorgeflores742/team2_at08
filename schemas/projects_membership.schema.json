{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "title": "The Root Schema",
  "required": [
    "kind",
    "id",
    "created_at",
    "updated_at",
    "person",
    "project_id",
    "role",
    "project_color",
    "favorite",
    "wants_comment_notification_emails",
    "will_receive_mention_notifications_or_emails"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "project_membership"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        10077186
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-02-12T23:19:44Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-02-12T23:19:44Z"
      ],
      "pattern": "^(.*)$"
    },
    "person": {
      "$id": "#/properties/person",
      "type": "object",
      "title": "The Person Schema",
      "required": [
        "kind",
        "id",
        "name",
        "email",
        "initials",
        "username"
      ],
      "properties": {
        "kind": {
          "$id": "#/properties/person/properties/kind",
          "type": "string",
          "title": "The Kind Schema",
          "default": "",
          "examples": [
            "person"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/person/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            3143772
          ]
        },
        "name": {
          "$id": "#/properties/person/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "wilsonlopez"
          ],
          "pattern": "^(.*)$"
        },
        "email": {
          "$id": "#/properties/person/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "wilson.lopez@fundacion-jala.org"
          ],
          "pattern": "^(.*)$"
        },
        "initials": {
          "$id": "#/properties/person/properties/initials",
          "type": "string",
          "title": "The Initials Schema",
          "default": "",
          "examples": [
            "WI"
          ],
          "pattern": "^(.*)$"
        },
        "username": {
          "$id": "#/properties/person/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "default": "",
          "examples": [
            "wilsonlopez1"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "project_id": {
      "$id": "#/properties/project_id",
      "type": "integer",
      "title": "The Project_id Schema",
      "default": 0,
      "examples": [
        2295414
      ]
    },
    "role": {
      "$id": "#/properties/role",
      "type": "string",
      "title": "The Role Schema",
      "default": "",
      "examples": [
        "member"
      ],
      "pattern": "^(.*)$"
    },
    "project_color": {
      "$id": "#/properties/project_color",
      "type": "string",
      "title": "The Project_color Schema",
      "default": "",
      "examples": [
        "e46642"
      ],
      "pattern": "^(.*)$"
    },
    "favorite": {
      "$id": "#/properties/favorite",
      "type": "boolean",
      "title": "The Favorite Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "wants_comment_notification_emails": {
      "$id": "#/properties/wants_comment_notification_emails",
      "type": "boolean",
      "title": "The Wants_comment_notification_emails Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "will_receive_mention_notifications_or_emails": {
      "$id": "#/properties/will_receive_mention_notifications_or_emails",
      "type": "boolean",
      "title": "The Will_receive_mention_notifications_or_emails Schema",
      "default": false,
      "examples": [
        true
      ]
    }
  }
}