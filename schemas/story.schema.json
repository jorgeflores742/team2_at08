{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "title": "The Root Schema",
  "required": [
    "kind",
    "id",
    "project_id",
    "name",
    "story_type",
    "current_state",
    "requested_by_id",
    "owner_ids",
    "labels",
    "created_at",
    "updated_at",
    "url"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "story"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        163851894
      ]
    },
    "project_id": {
      "$id": "#/properties/project_id",
      "type": "integer",
      "title": "The Project_id Schema",
      "default": 0,
      "examples": [
        2268425
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Story of acceptance"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "this is my description"
      ],
      "pattern": "^(.*)$"
    },
    "story_type": {
      "$id": "#/properties/story_type",
      "type": "string",
      "title": "The Story_type Schema",
      "default": "",
      "examples": [
        "feature"
      ],
      "pattern": "^(.*)$"
    },
    "current_state": {
      "$id": "#/properties/current_state",
      "type": "string",
      "title": "The Current_state Schema",
      "default": "",
      "examples": [
        "unscheduled"
      ],
      "pattern": "^(.*)$"
    },
    "estimate": {
      "$id": "#/properties/estimate",
      "type": "integer",
      "title": "The Estimate Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "requested_by_id": {
      "$id": "#/properties/requested_by_id",
      "type": "integer",
      "title": "The Requested_by_id Schema",
      "default": 0,
      "examples": [
        3145683
      ]
    },
    "owner_ids": {
      "$id": "#/properties/owner_ids",
      "type": "array",
      "title": "The Owner_ids Schema"
    },
    "owned_by_id": {
      "$id": "#/properties/owned_by_id",
      "type": "integer",
      "title": "The Owned_by_id Schema",
      "default": 0,
      "examples": [
        3142814
      ]
    },
    "labels": {
      "$id": "#/properties/labels",
      "type": "array",
      "title": "The Labels Schema"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-02-11T01:46:13Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-02-11T01:46:13Z"
      ],
      "pattern": "^(.*)$"
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "https://www.pivotaltracker.com/story/show/163851894"
      ],
      "pattern": "^(.*)$"
    }
  }
}
