{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "created_at",
    "id",
    "kind",
    "name",
    "plan",
    "status",
    "updated_at"
  ],
  "properties": {
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-01-29T12:00:00Z"
      ],
      "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        1081141
      ]
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "account"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "minLength": 1,
      "maxLength": 100,
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "new_acc"
      ],
      "pattern": "^(.*)$"
    },
    "plan": {
      "$id": "#/properties/plan",
      "type": "string",
      "title": "The Plan Schema",
      "default": "",
      "examples": [
        "Free"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "active"
      ],
      "pattern": "^(active|suspended|delinquent|deleted|limited)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-01-31T14:53:40Z"
      ],
      "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
    }
  },
  "additionalProperties": false
}